{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "indent": ["error", "tab"],
        "linebreak-style": ["error", "windows"],
        "no-tabs": 0,
        "max-len": 0,
        "no-invalid-this": 0,
        "one-var-declaration-per-line": ["error", "always"]
    }
}
